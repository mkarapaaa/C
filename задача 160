#include <stciio.h>
#include <conio.h>
void main() {
int n[6]; 
int s = 0; 
float p[6]; 
char *mes[6] ={"\0", "\0","двоек\О","троек\О", "четверок\О","пятерок\О"};
int i;
puts("Обработка результатов экзамена");
puts("Введите исходные данные:");
for (i = 5; i >= 2; i—)
{
  printf("%s ->", mes[i]); 
  scanf("%i", &n[i]);
  s +=n[i];
}
// вычислим процент каждой оценки 
for (i = 2; i < 6; i++)
  p[i] = (float)n[i]/3*100;
puts("Результаты экзамена");
puts("-----------------------------"); 
for (i = 5; i >= 2; i—-)
printf("%8s %2i %2.0f%\n",mes[i], n[i], p[i]); 
puts("------------------------------");
puts("Для завершения программы нажмите <Enter>");
getch(); 
}
