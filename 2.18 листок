#include <stdio.h>
#include <math.h>

int main() {
    float x = 5;
    float y = 10;
    float z, q;

    // Вычисление z
    z = (x + (2 + y) / (x * x)) / (y + 1 / sqrt(x * x + 10));

    // Вычисление q
    q = 7.2 * sin(x) - fabs(y);

    printf("Значение z: %f\n", z);
    printf("Значение q: %f\n", q);

    return 0;
}
