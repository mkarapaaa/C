#include <stdio.h>
#include <conio.h>
void mainO
{
int year;
printf("\nВ каком году бьш основан Санкт-Петербург?\n");
printf("Введите число и нажмите <Enter>");
printf("-> " ) ;
scanf("%i", &year);
if (year == 1703)
printf("Правильно.");
else {
printf("Вы ошиблись, " ) ;
printf ("Санкт-Петербург был основан в 1703 году\n");
}
printf("\nДля завершения нажмите <Enter>");
getch();
}
